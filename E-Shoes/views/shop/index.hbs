{{# each this}}
<div class="row">
  {{# each this}}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{this.imagePath}}" class="
      image-responsive ">
      <div class="caption">
        <h3>{{this.title}} </h3>
        <p>{{this.description}}}</p>
        <div>
            <div class="price">{{this.price}}</div>
            <a href="#" class="btn btn-success" role="button">Comprar</a>
        </div>
      </div>
    </div>
  </div>
    {{/each}}
</div>
{{/each}}